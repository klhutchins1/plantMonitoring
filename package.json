{
  "name": "plantMonitoring",
  "private": true,
  "version": "0.0.5",
  "description": "monitor your plants with raspberry pi and meteor",
  "author": "KH",
  "scripts": {
    "start": "meteor run",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "dependencies": {
    "ds18b20": "0.1.0",
    "onoff": "1.1.1",
    "rpi-dht-sensor": "0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0:",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-jsx-a11y": "^2.2.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "settings": {
      "import/resolver": "meteor"
    },
    "rules": {}
  }
}
